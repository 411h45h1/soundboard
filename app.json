{
  "expo": {
    "name": "SoundBoard",
    "slug": "SoundBoard",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#5E403F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/7b28c2b7-8f37-47d2-8d32-c0f189205b39"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.ourends.soundboard",
      "supportsTablet": true,
      "backgroundColor": "#5E403F",
      "infoPlist": {
        "UIBackgroundModes": ["audio"],
        "NSMicrophoneUsageDescription": "This app requires access to the microphone to record audio."
      }
    },
    "android": {
      "permissions": ["RECORD_AUDIO"],
      "package": "com.notso_software.soundboard",
      "backgroundColor": "#5E403F",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#5E403F"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7b28c2b7-8f37-47d2-8d32-c0f189205b39"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
