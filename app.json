{
  "expo": {
    "name": "SoundBoard",
    "slug": "SoundBoard",
    "scheme": "soundboard",
    "version": "2.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#5E403F"
    },
    "newArchEnabled": true,
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/7b28c2b7-8f37-47d2-8d32-c0f189205b39"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#5E403F",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "ios": {
      "bundleIdentifier": "com.ourends.soundboard",
      "supportsTablet": true,
      "backgroundColor": "#5E403F",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "audio"
        ],
        "NSMicrophoneUsageDescription": "This app requires access to the microphone to record audio.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "permissions": [
        "RECORD_AUDIO"
      ],
      "package": "com.notso_software.soundboard",
      "backgroundColor": "#5E403F",
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#5E403F"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7b28c2b7-8f37-47d2-8d32-c0f189205b39"
      }
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}