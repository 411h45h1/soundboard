{
  "main": "node_modules/expo/AppEntry.js",
  "version": "1.2.0",
  "license": "UNLICENSED",
  "scripts": {
    "reinstall:start": "bun ghd && bun start",
    "ghd": "bun clean && bun i",
    "start": "expo start --dev-client",
    "start:c": "expo start -c --dev-client",
    "clean": "rm -rf dist .expo .expo-shared node_modules yarn.lock package-lock.json bun.lock",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "publish": "expo publish",
    "publish:web": "expo build:web && netlify deploy --prod",
    "build": "eas build --platform all",
    "build:submit:ios": "bun build:ios && bun submit:ios",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:dev:android": "eas build --profile development --platform android",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "submit:ios": "eas submit -p ios",
    "submit:android": "eas submit -p android",
    "submit:all": "eas submit -p all",
    "build:submit:all": "bun build && bun submit:all",
    "update:branch:prod": "eas update --branch prod --auto",
    "update:branch:stg": "eas update --branch stg --auto"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "expo": "^51.0.34",
    "expo-av": "~14.0.7",
    "expo-dev-client": "~4.0.28",
    "expo-document-picker": "~12.0.2",
    "expo-file-system": "~17.0.1",
    "expo-haptics": "~13.0.1",
    "expo-splash-screen": "~0.27.6",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.7",
    "expo-updates": "~0.25.25",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-web": "~0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0"
  },
  "private": true
}
