{
  "main": "node_modules/expo/AppEntry.js",
  "version": "1.3.0",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "reinstall:start": "bun clean:install && bun start",
    "start": "expo start",
    "start:c": "expo start -c",
    "clean:setup": "bun clean:install && bun format && bun lint && bun type-check",
    "clean:install": "bun clean && bun install",
    "clean": "rm -rf dist .expo .expo-shared node_modules yarn.lock package-lock.json bun.lock",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "build": "eas build --platform all",
    "build:submit:ios": "bun build:ios && bun submit:ios",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:dev:android": "eas build --profile development --platform android",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "submit:ios": "eas submit -p ios",
    "submit:android": "eas submit -p android",
    "submit:all": "eas submit -p all",
    "build:submit:all": "bun build && bun submit:all",
    "update:branch:prod": "eas update --branch prod --auto",
    "update:branch:stg": "eas update --branch stg --auto"
  },
  "dependencies": {
    "@expo/ui": "~0.2.0-beta.7",
    "@react-native-async-storage/async-storage": "2.2.0",
    "expo": "^54.0.13",
    "expo-av": "~16.0.7",
    "expo-dev-client": "~6.0.15",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.17",
    "expo-font": "~14.0.9",
    "expo-glass-effect": "~0.1.4",
    "expo-haptics": "~15.0.7",
    "expo-router": "^6.0.12",
    "expo-screen-orientation": "~9.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~29.0.12",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.75.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/jest": "29.5.14",
    "@types/react": "~18.3.11",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "~29.7.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  },
  "private": true
}
