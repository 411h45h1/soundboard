{
  "main": "node_modules/expo/AppEntry.js",
  "version": "1.2.0",
  "license": "UNLICENSED",
  "scripts": {
    "reinstall:start": "yarn ghd && yarn start",
    "ghd": "yarn clean && yarn",
    "start": "expo start --dev-client",
    "clean": "rimraf .expo .expo-shared node_modules yarn.lock",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "publish": "expo publish",
    "publish:web": "expo build:web && netlify deploy --prod",
    "build": "eas build --platform all",
    "build:submit:ios": "yarn build:ios && yarn submit:ios",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:dev:android": "eas build --profile development --platform android",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "submit:ios": "eas submit -p ios",
    "submit:android": "eas submit -p android",
    "submit:all": "eas submit -p all",
    "build:submit:all": "yarn build && yarn submit:all",
    "update:branch:prod": "eas update --branch prod --auto",
    "update:branch:stg": "eas update --branch stg --auto"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "expo": "^52.0.41",
    "expo-av": "~15.0.2",
    "expo-dev-client": "~5.0.15",
    "expo-document-picker": "~13.0.3",
    "expo-file-system": "~18.0.12",
    "expo-haptics": "~14.0.1",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.8",
    "expo-updates": "~0.27.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0"
  },
  "private": true
}
